{
    "name": "bot-2D",
    "description": "nodejs whatsapp bot",
    "logo": "https://i.ibb.co/sWmwD0k/e0bslrup.jpg",
    "repository": "https://github.com/jacktheboss220/WhatsAppBotMultiDevice",
    "keywords": [
        "whatsapp-bot",
        "insta",
        "multi-device"
    ],
    "env": {
        "REMOVE_BG_KEY": {
            "description": "Get from remove.bg"
        },
        "DEEPAI_KEY": {
            "description": "Get your api key from deepai.org"
        },
        "myNumber": {
            "description": "+32491145572"
        },
        "INSTA_API_KEY": {
            "description": "Enter insta session key."
        },
        "TRUECALLER_ID": {
            "description": "Enter true caller id"
        },
        "MONGODB_KEY": {
            "description": "Enter the mongodb username:password"
        },
        "LYRICS_KEY": {
            "description": "Enter genius api key"
        }
    },
    "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
        }
    ],
    "addons": [
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "13"
            }
        }
    ]
}
